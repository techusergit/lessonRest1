{
  "mappings": [
    {
      "priority": 1,
      "request": {
        "method": "GET",
        "url": "/api/mytest"
      },
      "response": {
        "status": 200,
        "body": "Response no params",
        "headers": {
          "Content-Type": "text/plain"
        }
      }
    },
    {
      "priority": 1,
      "request": {
        "method": "GET",
        "url": "/api/mytest?param1=val1"
      },
      "response": {
        "status": 200,
        "body": "Response with params!",
        "headers": {
          "Content-Type": "text/plain"
        }
      }
    },
    {
      "request": {
        "method": "POST",
        "url": "/api/params",
        "headers": {
          "Content-Type": {
            "matches": "application/json"
          }
        },
        "bodyPatterns" : [ {
          "equalToJson" : "{ \"paramId\" :34 }"
        } ]
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "{\"status\":\"Updated\"}"
      }
    }
  ]
}